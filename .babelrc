{
  "presets": [
    [
      "env", {
        "useBuiltIns": true
      }
    ],
    "stage-2",
    "react"
  ],
  "plugins": [
    "lodash",
    [
      "module-resolver", {
        "root": [
          "."
        ],
        "alias": {
          "react": "preact-compat",
          "react-dom": "preact-compat"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "presets": [
        [
          "minify", {
            "mangle": false
          }
        ]
      ]
    }
  }
}

