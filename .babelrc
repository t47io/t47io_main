{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    "lodash",
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "react": "preact-compat",
        "react-dom": "preact-compat"
      }
    }]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "production": {
      "presets": [
        ["babili", { "mangle": false }]
      ]
    }
  }
}

