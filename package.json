{
  "name": "t47io_main",
  "version": "3.7.4",
  "description": "Personal website app of t47.io",
  "main": "server/server.js",
  "engines": {
    "node": "^12.16.3",
    "yarn": "^1.22.4"
  },
  "scripts": {
    "dev": "rm -rf ./public/*.html* && yarn prep && yarn nodemon",
    "prod": "pm2 start --interpreter babel-node server/server.js -n main",

    "prep": "concurrently --raw \"yarn build:json\" \"yarn build:temp\"",
    "stat": "yarn cron && yarn build && yarn cron:chmod",
    "nodemon": "nodemon --config .nodemonrc server/server.js",

    "lint": "concurrently --raw \"yarn lint:js\" \"yarn lint:css\"",
    "lint:js": "eslint applications build server webpack.*.js --ext [.jsx,.js] --fix",
    "lint:css": "stylefmt -i applications/common/mixins/sprite.scss -r \"applications/**/*.{css,scss}\"",

    "build": "yarn lint:js && yarn prep && yarn build:server && yarn build:client",
    "build:json": "babel-node ./build/json.js",
    "build:temp": "yarn webpack:temp && concurrently --raw \"yarn process:temp\" \"yarn process:error\"",
    "build:server": "yarn webpack:server && yarn process:server",
    "build:client": "yarn webpack:client && yarn process",
    "webpack:temp": "BABEL_ENV=production babel-node ./node_modules/webpack/bin/webpack.js --config webpack.config.temp.js",
    "webpack:server": "BABEL_ENV=production babel-node ./node_modules/webpack/bin/webpack.js --config webpack.config.server.js",
    "webpack:client": "BABEL_ENV=production babel-node ./node_modules/webpack/bin/webpack.js --config webpack.config.client.js --verbose",

    "process": "yarn process:client && concurrently --raw \"yarn process:manifest\" \"yarn process:static\"",
    "process:temp": "babel-node ./build/process/temp.js",
    "process:server": "babel-node ./build/process/server.jsx",
    "process:client": "babel-node ./build/process/client.js",
    "process:error": "babel-node ./build/process/error.js",
    "process:manifest": "babel-node ./build/process/manifest.js",
    "process:static": "concurrently --raw \"yarn process:copy\" \"yarn process:sitemap\" && yarn process:clean",
    "process:copy": "babel-node ./build/process/copy.js",
    "process:sitemap": "babel-node ./build/process/sitemap.js",
    "process:clean": "babel-node ./build/process/clean.js",

    "cron": "concurrently --raw \"yarn cron:contrib\" \"yarn cron:repo\" \"yarn cron:scholar\" \"yarn cron:https\" && yarn cron:backup && yarn cron:email",
    "cron:contrib": "babel-node ./server/cron/git_contrib.js",
    "cron:repo": "babel-node ./server/cron/git_repo.js",
    "cron:scholar": "babel-node ./server/cron/cite_scholar.js",
    "cron:https": "babel-node ./server/cron/ssl_expire.js",
    "cron:backup": "babel-node ./server/cron/backup.js",
    "cron:email": "babel-node ./server/cron/email.js",
    "cron:chmod": "babel-node ./server/cron/chmod.js",
    "cron:renew": "babel-node ./server/cron/ssl_renew.js",

    "update": "yarn upgrade-interactive --exact && yarn autoclean --force",
    "update:nginx": "babel-node ./server/cron/build_nginx.js"
  },
  "repository": "github:t47io/t47io_main",
  "license": "CC-BY-NC-SA-4.0",
  "author": "Siqi Tian <contact@t47.io>",
  "homepage": "https://t47.io/",
  "keywords": [
    "t47io",
    "personal",
    "portfolio"
  ],
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
