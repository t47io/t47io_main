{
  "name": "t47io_main",
  "version": "2.1.3",
  "description": "Personal website assets of t47.io",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,jsx,json,css,scss,html -i public/ -i config/main.json -x babel-node -d 3 -V server/server.js",
    "build": "BABEL_ENV=production babel-node ./node_modules/webpack/bin/webpack.js --verbose",
    "postbuild": "npm run process",
    "update": "npm-check-updates -u -a",
    "lint": "./node_modules/eslint/bin/eslint.js applications build server webpack.*.js --ext [.jsx,.js]",
    "process": "npm run process:main && npm run process:error",
    "process:main": "babel-node ./build/main.js",
    "process:error": "babel-node ./build/error.jsx",
    "cron": "npm run cron:contrib && npm run cron:scholar && npm run cron:https && npm run cron:report",
    "cron:contrib": "babel-node ./server/cron/git_contrib.js",
    "cron:scholar": "babel-node ./server/cron/cite_scholar.js",
    "cron:https": "babel-node ./server/cron/ssl_expire.js",
    "cron:report": "babel-node ./server/cron/data_report.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t47io/t47io_main"
  },
  "author": "Siqi Tian <contact@t47.io>",
  "license": "CC-BY-NC-SA",
  "homepage": "http://t47.io/",
  "keywords": [
    "t47io",
    "personal",
    "portfolio"
  ],
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "bootstrap-loader": "^2.0.0",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
